PRAGMA table_info(sales);
SELECT &quot;Region&quot; FROM sales;
SELECT * FROM sales
WHERE &quot;Region&quot; = 'West'
ORDER BY &quot;Sales_Amount&quot; DESC;
SELECT &quot;Region&quot;, SUM(&quot;Sales_Amount&quot;) AS Total_Sales
FROM sales
GROUP BY &quot;Region&quot;
ORDER BY &quot;Total_Sales&quot; DESC;
SELECT &quot;Product_Category&quot;, AVG(&quot;Unit_Price&quot;) AS &quot;Avg_Unit_Price&quot;
FROM sales
GROUP BY &quot;Product_Category&quot;
ORDER BY &quot;Avg_Unit_Price&quot; DESC;
SELECT * FROM sales
WHERE &quot;Sales_Amount&quot; &gt; (SELECT AVG(&quot;Sales_Amount&quot;) FROM sales);

SELECT &quot;Sales_Rep&quot;, SUM(&quot;Sales_Amount&quot;) AS &quot;Total_Sales&quot;
FROM sales
GROUP BY &quot;Sales_Rep&quot;
ORDER BY &quot;Total_Sales&quot; DESC
LIMIT 5;




SELECT &quot;Customer_Type&quot;, &quot;Payment_Method&quot;, COUNT(*) AS Total_Transactions
FROM sales
GROUP BY &quot;Customer_Type&quot;, &quot;Payment_Method&quot;
ORDER BY Total_Transactions DESC;

SELECT &quot;Product_Category&quot;, AVG(&quot;Discount&quot;) AS Avg_Discount
FROM sales
GROUP BY &quot;Product_Category&quot;
ORDER BY Avg_Discount DESC;



